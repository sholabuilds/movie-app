 <header>
    <nav>
      <%# <img src="app/assets/images/logo.png" alt="Flix logo"> %>
      <%= link_to image_tag("all_flix_logo.png"), root_path %>
      <ul class="left">
        <li><%= link_to "All movies", movies_path %></li>
      </ul>
      <ul class="right">
        <% if current_user %>
          <%= link_to current_user.username, current_user %>
          <li><%= link_to "Sign out", signin_path, class: "button" %></li>
        <% else %>%
          <li><%= link_to "Sign in", signin_path, class: "button" %></li>
          <li><%= link_to "Sign up", signup_path, class: "button" %></li>
        <% end %>
      </ul>
    </nav>
  </header>